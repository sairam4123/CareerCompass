{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@4.6.0",
      "maxDuration": 10,
      "memory": 256,
      "includeFiles": "api/**"
    }
  },
  "devCommand": "cd frontend && yarn dev",
  "installCommand": "cd frontend && yarn install && cd ../api && ",
  "buildCommand": "cd frontend && yarn run build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/$1"
    }
  ],
  "outputDirectory": "frontend/dist"
}
