{
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@4.6.0",
      "maxDuration": 10,
      "memory": 256,
      "includeFiles": "server/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/$1"
    }
  ]
}
